    <!-- Start: Login Form Basic -->
    <section class="position-relative py-4 py-xl-5">
      <div class="container">
        <form class="text-center" method="POST" action="/itemadd">
        <div class="row d-flex justify-content-center" style="margin-top: 100px;">
          <div class="col-md-6 col-xl-4">
            <div class="card mb-5">
              <div class="card-body d-flex flex-column align-items-center">
                
                  <div class="mb-3">
                    <input
                      class="form-control"
                      type="text"
                      placeholder="Item Code"
                      name="id"
                      required
                    />
                  </div>
                  <div class="mb-3">
                    <input
                      class="form-control"
                      type="text"
                      placeholder="Items"
                      name="name"
                      required
                    />
                  </div>
                  <div class="mb-3">
                    <input
                      class="form-control"
                      type="text"
                      placeholder="Price"
                      name="price"
                      required
                    />
                  </div>
                  <div class="mb-3">
                    <input
                      class="form-control"
                      type="text"
                      placeholder="Available Stock"
                      name="stock"
                      required
                    />
                  </div>
                <button class="btn btn-primary d-block w-100" type="submit">
                  <%=type%>
                </button>
              </div>
            </div>
          </div>
        
        </div>
    </form>
      </div>
